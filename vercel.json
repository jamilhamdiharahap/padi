{
 "version": 2,
 "builds": [
     {
         "src": "app.js",
         "use": "@vercel/node"
     }
 ],
 "routes": [
     {
         "src": "/(.*)",
         "dest": "/app.js"
     }
 ],
 "functions": {
    "scheduled-function": {
      "file": "schedular.js",
      "schedule": "cron(0 0 * * *)"
    }
  }
 
}